.jimu-widget-select {
	width: 100%;
	height: 100%;
	overflow: hidden;
}

.jimu-widget-select .jimu-viewstack {
	height: 100%;
}

.jimu-widget-select .jimu-viewstack .view {
	position: relative;
}

.jimu-widget-select .seperator {
	width: 100%;
	height: 1px;
	background: #D7D7D7;
	margin: 20px 0;
}

.jimu-widget-select .title {
	font-family: "Avenir Light";
	font-size: 12px;
	line-height: 16px;
	color: #000000;
	margin: 20px 0;
}

.jimu-widget-select .normal-label,
.jimu-widget-select .light-label,
.jimu-widget-select .selectable-layer-item .layer-row .layer-name,
.jimu-widget-select .selectable-layer-item .layer-row .selected-num,
.jimu-widget-select .medium-label {
	line-height: 36px;
	height: 36px;
	font-size: 12px;
	color: #000000;
}

.jimu-widget-select .light-label,
.jimu-widget-select .selectable-layer-item .layer-row .layer-name,
.jimu-widget-select .selectable-layer-item .layer-row .selected-num {
	font-family: "Avenir Light";
}

.jimu-widget-select .medium-label {
	font-family: "Avenir Medium";
}

.jimu-widget-select .layer-nodes {
	position: absolute;
	top: 65px;
	bottom: 0;
	width: 100%;
	overflow: auto;
}


